<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @school.name %>
</p>

<p>
  <strong>Start Time:</strong>
  <%if @school.start_time%>
    <%= (@school.start_time).strftime("%r") %>
  <%else%>
    N/A
  <%end%>
</p>

<p>
  <strong>Address:</strong>
  <%= @school.address %>
</p>

<%= link_to 'Edit', edit_school_path(@school), class: 'button', style: 'color: white' %>
<%= link_to 'Back', schools_path, class: 'button', style: 'color: white' %>
